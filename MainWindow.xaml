<Window x:Class="Harley.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="Angle1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" TextWrapping="Wrap" Text="TextBox" Width="120"/>
        <Viewbox Grid.Row="1" Stretch="Uniform" HorizontalAlignment="Center">
            <Grid>
                <Image Name="Image" Width="640" Height="480"/>
                <Canvas x:Name="videoCanvas" Width="640" Height="480"/>
            </Grid>
        </Viewbox>
    </Grid>
</Window>
